extends header

block content
  .w3-card-4 
    header.w3-container-w3-teal
      h1 Inquirição modificada por #{i.editor}
    .w3-container
        fieldset
            legend Metadata
            label #[b id: ] #{i._id}
            br
            label #[b UnitId: ] #{i.UnitId}
            br
            label #[b UnitTitle: ] #{i.UnitTitle}
            br
            label #[b UnitDateInitial: ] #{i.UnitDateInitial}
            br
            label #[b UnitDateFinal: ] #{i.UnitDateFinal}
            br
            label #[b UnitDateInitialCertainty: ] #{i.UnitDateInitialCertainty}
            br
            label #[b UnitDateFinalCertainty: ] #{i.UnitDateFinalCertainty}
            br
            label #[b AllowUnitDatesInference: ] #{i.AllowUnitDatesInference}
            br
            label #[b Dimensions: ] #{i.Dimensions}
            br
            label #[b AccessRestrict: ] #{i.AccessRestrict}
            br
            label #[b PhysLoc: ] #{i.PhysLoc}
            br
            label #[b PreviousLoc: ] #{i.PreviousLoc}
            br
            label #[b PhysTech: ] #{i.PhysTech}
            br
            label #[b RelatedMaterial: ] #{i.RelatedMaterial}
            br
            label #[b Note: ] #{i.Note}
            br
            label #[b Revised: ] #{i.Revised}
            br
            label #[b Published: ] #{i.Published}
            br
            label #[b Available: ] #{i.Available}
            br
            label #[b Creator: ] #{i.Creator}
            br
            label #[b Created: ] #{i.Created}
            br
            label #[b Username: ] #{i.Username}
            br
            label #[b ProcessInfoDate: ] #{i.ProcessInfoDate}
            br
            label #[b ProcessInfo: ] #{i.ProcessInfo}
            br
            label #[b filiations: ] #{i.filiations}
            br
            label #[b birthplace: ] #{i.birthplace}
            br
            label #[b current_concelho: ] #{i.current_concelho}
            br
            label #[b current_district: ] #{i.current_district}
            br
            label #[b relations_id: ]
            .w3-container
              each relation in i.relations_id
                b #{relation.name}
                br
                label #[b Relação] : #{relation.type}
                br
                a(href="/inquiry/"+relation.id) link
                br
            br

    footer.w3-indigo 
      address Generated by auth:RPCW2023 #{d} | #[a(href="/") Voltar]